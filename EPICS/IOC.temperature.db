record(ai, "$(DEVICE):t1:gettemp") {
    field(DESC, "Get T1 temperature")
    field(DTYP, "stream")
    field(INP, "@libcom1.proto com1_getLink(t1) $(PORT)")
    field(SCAN. "1 second")
    field(PRIO, "LOW")
}

