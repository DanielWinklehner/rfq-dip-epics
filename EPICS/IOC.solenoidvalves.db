record(bi, "$(DEVICE):s1:get") {
    field(DESC, "Get solenoid 1 status")
    field(DTYP, "stream")
    field(INP, "@libcom1.proto com1_getLink(s1) $(PORT)")
    field(SCAN, "0.1 second")
}

record(bo, "$(DEVICE):s1:set") {
    field(DESC, "Set solenoid 1 on")
    field(DTYP, "stream")
    field(OUT, "@libcom1.proto com1.setLink(s1, 1) $(PORT)")
}

record(bo, "$(DEVICE):s1:off") {
    field(DESC, "Set solenoid 1 off"
}
