record(bi, "$(DEVICE):i1:get_interlock") {
    field(DESC, "Get interlock 1 status")
    field(DTYP, "stream")
    field(INP, "@libcom1.proto com1_getLink(i1) $(PORT)")
    field(SCAN, "0.1 second")    
    field(PRIO, "HIGH")
}

record(bi, "$(DEVICE):i2:get_interlock") {
    field(DESC, "Get interlock 2 status")
    field(DTYP, "stream")
    field(INP, "@libcom1.proto com1_getLink(i2) $(PORT)")
    field(SCAN, "0.1 second")
    field(PRIO, "HIGH")
}

record (bo "$(DEVICE):s1:set_solenoid") {
    field(DESC, "Set solenoid 1")
    field(DTYP, "stream")
    field(OUT, "@libcom1.proto com1_setLink(s1) $(PORT)")
}

record (bo "$(DEVICE):s2:set_solenoid") {
    field(DESC, "Set solenoid 2")
    field(DTYP, "stream")
    field(OUT, "@libcom1.proto com1_setLink(s2) $(PORT)")
}
