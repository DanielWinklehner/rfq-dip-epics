record(aai, "$(DEVICE):data") {
    field(DESC, "All pressures")
    field(DTYP, "stream")
    field(INP, "@libcom1.proto read_all_array $(PORT)")
    field(SCAN, ".1 second")
    field(NELM, "2")
    field(FTVL, "DOUBLE")
    field(FLNK, "$(DEVICE):int:fanout.PROC PP")
}

record(fanout, "$(DEVICE):int:fanout") {
    field(SELM, "All")
    field(LNK0, "$(DEVICE):apg:getpressure")
    field(LNK1, "$(DEVICE):diag_box:getpressure")
}

record(ai, "$(DEVICE):apg:getpressure") {
    field(DESC, "Get APG Pressure")
    field(INP, "$(DEVICE):data.VAL[0] CA")
    field(SCAN, "Passive")
}

record(ai, "$(DEVICE):diag_box:getpressure") {
    field(DESC, "Get Diagnostic Box Pressure")
    field(INP, "$(DEVICE):data.VAL[1] CA")
    field(SCAN, "Passive")
}
